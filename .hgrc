[alias]
vlog = log --template '\033[0;33mcommit: {rev}:{node}\n\033[0mAuthor: {author}\nDate:   {date|rfc822date}\n\n\t{desc|strip|fill68|tabindent}\n\n'
log = log -v


[pager]
pager = /usr/share/git/diff-highlight/diff-highlight| less -+FX -R
attend = vlog, annotate, cat, diff, export, glog, log, qdiff

[extensions]
color =
pager =
