#!/bin/bash

glob="$(printf "%s" {A..z} "@?>=<;:" {0..9} "\/.-,+*)(\'&%\$#\"!")"
#glob="$(printf "%s" {z..A} "@?>=<;:" {0..9} "\/.-,+*)(\'&%\$#\"!")"

